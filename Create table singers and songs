SELECT * FROM artists;

SELECT stage_name, title
FROM artists, songs
WHERE artists.id=songs.artist_id;

SELECT real_name, stage_name, album,title
FROM artists
LEFT OUTER JOIN songs
ON artists.id = songs.artist_id;

SELECT stage_name, album, title, release_year,genre
FROM artists
JOIN songs
ON artists.id =songs.artist_id;
