CREATE TABLE artists (
 id INTEGER PRIMARY KEY AUTOINCREMENT,
 stage_name TEXT, 
 real_name TEXT,
 age TEXT,
 genre TEXT
);

INSERT INTO artists (stage_name, real_name,age,genre) VALUES ("Taylor Swift",  "Taylor Swift", "33","Pop");
