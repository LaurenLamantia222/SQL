INSERT INTO artists (stage_name, real_name) VALUES ("Kendrick Lamar", "Kendrick Lamar Duckworth");
INSERT INTO artists (stage_name,real_name) VALUES ("Eminem", "Marshall Bruce Mathers III");

CREATE TABLE songs (
 id INTEGER PRIMARY KEY AUTOINCREMENT,
  artist_id INTEGER,
  title TEXT,
  release_year INTEGER,
  album TEXT
 );
  
